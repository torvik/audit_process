<p id="notice"><%= notice %></p>

<h1>Import Form Excel</h1>
<%= form_tag import_from_excel_path, multipart: true do %>
 <%= file_field_tag :file %>
 <%= submit_tag "Import", :class=> "btn"%>
<% end %>

<h1>Actionfolders</h1>

<table>
  <thead>
    <tr>
      <th>Identifier</th>
      <th>Folder</th>
      <th>Lawyer</th>
      <th>Low date</th>
      <th>Closing date</th>
      <th>Status</th>
      <th>Action result</th>
      <th>Uf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actionfolders.each do |actionfolder| %>
      <tr>
        <td><%= actionfolder.identifier %></td>
        <td><%= actionfolder.folder %></td>
        <td><%= actionfolder.lawyer %></td>
        <td><%= actionfolder.low_date %></td>
        <td><%= actionfolder.closing_date %></td>
        <td><%= actionfolder.status %></td>
        <td><%= actionfolder.action_result %></td>
        <td><%= actionfolder.uf %></td>
        <td><%= link_to 'Show', actionfolder %></td>
        <td><%= link_to 'Edit', edit_actionfolder_path(actionfolder) %></td>
        <td><%= link_to 'Destroy', actionfolder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actionfolder', new_actionfolder_path %>
